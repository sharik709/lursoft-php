{
    "name": "sharik709/lursoft-php",
    "description": "PHP client for Lursoft API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sharik Shaikh",
            "email": "shaikhsharik709@gmail.com",
            "homepage": "https://github.com/sharik709"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "phpunit/phpunit": "^9.5",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Lursoft\\LursoftPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lursoft\\LursoftPhp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lursoft\\LursoftPhp\\Providers\\LursoftServiceProvider"
            ],
            "aliases": {
                "Lursoft": "Lursoft\\LursoftPhp\\Facades\\Lursoft"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "cs": "phpcs",
        "cs-fix": "phpcbf"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
